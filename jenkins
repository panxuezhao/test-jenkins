pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo "hello world!"
            }
        }
        stage('send mail to pxz') {
            steps {
                mail bcc: '', body: '张三 李四 王五 赵六', cc: '', from: '', replyTo: '', subject: 'test', to: '2263845092@qq.com'
            }
        }
    }
}
