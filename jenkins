pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo "hello world!"
            }
        }
        stage('send mail to pxz') {
            steps {
               sh 'mail bcc: '', body: '测试jenkins的pipeline', cc: '', from: '', replyTo: '', subject: 'test-pipeline', to: 'panxuezhao@iwgame.com''
            }
        }
    }
}
